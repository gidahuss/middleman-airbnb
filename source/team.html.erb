<ul class="list-inline text-center">
  <% data.users.each do |user| %>

    <li>
      <%= image_tag "https://kitt.lewagon.com/placeholder/users/#{user.github_username}", class: "img-circle" %>
      <h3><%= user.name.capitalize %></h3>
      <% link_to "http://github.com/#{user.github_username}" do %>
        <i class="fa fa-github"></i> <%= user.github_username %>
      <% end %>
    </li>

  <% end %>
</ul>



